<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<RootNamespace>Zord.Repository</RootNamespace>
	<AssemblyName>Zord.Repository.EntityFrameworkCore.Cache</AssemblyName>
	<Title>Zord.Repository.EntityFrameworkCore.Cache</Title>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<Authors>minhvu</Authors>
	<Description>EF Core Repository with auto load data to cache plugin package.</Description>
	<Summary>EF Core Repository with auto load data to cache plugin package.</Summary>
	<Version>7.0.0.1</Version>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
	<PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="7.0.10" />
	<PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Extensions.Caching\Extensions.Caching.csproj" />
    <ProjectReference Include="..\Repository.EfCore\Repository.EfCore.csproj" />
  </ItemGroup>

</Project>
